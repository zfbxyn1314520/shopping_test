package com.shopping.foundation.domain.api;

import java.util.List;

public class ApiGoodsSpecification {
		
	private long spec_id;
	
	private String spec_name;
	
	private List<Object> aproperty;

	public long getSpec_id() {
		return spec_id;
	}

	public void setSpec_id(long spec_id) {
		this.spec_id = spec_id;
	}

	public String getSpec_name() {
		return spec_name;
	}

	public void setSpec_name(String spec_name) {
		this.spec_name = spec_name;
	}

	public List<Object> getAproperty() {
		return aproperty;
	}

	public void setAproperty(List<Object> aproperty) {
		this.aproperty = aproperty;
	}

	public ApiGoodsSpecification() {
		super();
	}

	public ApiGoodsSpecification(long spec_id, String spec_name, List<Object> aproperty) {
		super();
		this.spec_id = spec_id;
		this.spec_name = spec_name;
		this.aproperty = aproperty;
	}

	@Override
	public String toString() {
		return "ApiGoodsSpecification [spec_id=" + spec_id + ", spec_name=" + spec_name + ", aproperty=" + aproperty
				+ "]";
	}

}
